      </local>
    </RunParams>
    <Units Count="4">
      <Unit0>
        <Filename Value="ftp.dpr"/>
        <IsPartOfProject Value="True"/>
      </Unit0>
      <Unit1>
        <Filename Value="ftputils.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="FtpUtils"/>
      </Unit1>
      <Unit2>
        <Filename Value="FtpConfDlg.pas"/>
        <IsPartOfProject Value="True"/>
        <ComponentName Value="DialogBox"/>
        <HasResources Value="True"/>
        <ResourceBaseClass Value="Form"/>
        <UnitName Value="FtpConfDlg"/>
      </Unit2>
      <Unit3>
        <Filename Value="ftpfunc.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="FtpFunc"/>
      </Unit3>
    </Units>
  </ProjectOptions>
  <CompilerOptions>
    <Version Value="8"/>
    <PathDelim Value="\"/>
    <SearchPaths>
      <IncludeFiles Value="$(ProjOutDir)\"/>
      <OtherUnitFiles Value="..\synapse\;..\..\..\..\sdk\"/>
      <UnitOutputDirectory Value="..\lib"/>
    </SearchPaths>
    <Parsing>
      <SyntaxOptions>
        <SyntaxMode Value="Delphi"/>
        <UseAnsiStrings Value="True"/>
      </SyntaxOptions>
    </Parsing>
    <Linking>
      <LinkSmart Value="True"/>
      <Options>
        <ExecutableType Value="Library"/>
      </Options>
    </Linking>
    <Other>
      <CustomOptions Value="-fPIC"/>
      <CompilerPath Value="$(CompPath)"/>
    </Other>
  </CompilerOptions>
</CONFIG>

      </Unit3>
      <Unit4>
        <Filename Value="ftpadv.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="FtpAdv"/>
      </Unit4>
      <Unit5>
        <Filename Value="sftp\sftpsend.pas"/>
        <IsPartOfProject Value="True"/>
        <UnitName Value="SftpSend"/>
      </Unit5>

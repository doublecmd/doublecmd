      <LinkSmart Value="True"/>
      <Options>
        <PassLinkerOptions Value="True"/>
        <ExecutableType Value="Library"/>
      </Options>
    </Linking>
    <Other>
      <Verbosity>
        <ShowNotes Value="False"/>
        <ShowHints Value="False"/>
      </Verbosity>
    </Other>
  </CompilerOptions>
</CONFIG>
